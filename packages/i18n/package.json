{
  "name": "@optum/i18n",
  "version": "1.0.0",
  "description": "Build Translation",
  "type": "module",
  "imports": {
    "@": "./src"
  },
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "bin": {
    "optum-i18n": "./dist/cli.js"
  },
  "scripts": {
    "build": "npm run type-check && pkgroll --minify -p tsconfig.prod.json --env.NODE_ENV=production",
    "dev": "pkgroll -p tsconfig.prod.json --env.NODE_ENV=development --watch",
    "link": "npm run build && npm link -f",
    "start": "node ./dist/cli.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Banisetti Naveen Kumar",
  "license": "UNLICENSED",
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@langchain/core": "^0.3.40",
    "@langchain/openai": "^0.4.4",
    "@lobehub/cli-ui": "^1.10.1",
    "@optum/eslint-config": "workspace:*",
    "@optum/typescript-config": "workspace:*",
    "@types/json-stable-stringify": "^1.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "18.3.1",
    "@types/update-notifier": "^6.0.8",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "conf": "^13.1.0",
    "consola": "^3.4.0",
    "cosmiconfig": "^9.0.0",
    "dirty-json": "^0.9.2",
    "dotenv": "^16.4.7",
    "fast-deep-equal": "^3.1.3",
    "glob": "^11.0.1",
    "gpt-tokenizer": "^2.8.1",
    "gray-matter": "^4.0.3",
    "ink": "^5.1.0",
    "json-stable-stringify": "^1.2.1",
    "just-diff": "^6.0.2",
    "langchain": "^0.3.15",
    "lodash-es": "^4.17.21",
    "p-map": "^7.0.3",
    "pangu": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "start": "^5.1.0",
    "swr": "^2.3.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "update-notifier": "^7.3.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/unist": "^3.0.3",
    "pkgroll": "^2.10.0",
    "tsup": "^8.3.6"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.1"
    }
  }
}
